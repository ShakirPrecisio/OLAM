                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_weight="1">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/_12sdp"
                                android:text="@string/price"
                                android:textColor="@color/colorPrimaryDark"
                                android:fontFamily="@font/poppins_semibold"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:layout_centerInParent="true"
                                android:layout_centerHorizontal="true"/>
                            <LinearLayout
                                android:id="@+id/linPrice"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginTop="@dimen/_6sdp"
                                android:layout_marginStart="@dimen/_3sdp"
                                android:layout_marginEnd="@dimen/_3sdp"
                                android:background="@drawable/bg_for_editest">
                                <EditText
                                    android:id="@+id/edtPrice"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/txt_spinner_color"
                                    android:fontFamily="@font/poppins_regular"
                                    android:layout_gravity="center"
                                    android:singleLine="true"
                                    android:maxLength="10"
                                    android:inputType="number"
                                    android:textSize="@dimen/_11sdp"
                                    android:drawableTint="@color/bg_arrow_tint"
                                    android:layout_marginLeft="@dimen/_9sdp"
                                    android:layout_marginRight="@dimen/_9sdp"
                                    android:textColorHint="@color/txt_spinner_color"
                                    android:backgroundTint="@color/white"
                                    />
                            </LinearLayout>
                        </LinearLayout>


                            <LinearLayout
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:layout_weight="1">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/_12sdp"
                                    android:text="@string/refraction"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:fontFamily="@font/poppins_semibold"
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"/>
                                <LinearLayout
                                    android:id="@+id/linRefraction"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_marginEnd="@dimen/_3sdp"
                                    android:background="@drawable/bg_for_editest">
                                    <EditText
                                        android:id="@+id/edtRefraction"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/txt_spinner_color"
                                        android:fontFamily="@font/poppins_regular"
                                        android:layout_gravity="center"
                                        android:singleLine="true"
                                        android:maxLength="2"
                                        android:inputType="number"
                                        android:textSize="@dimen/_11sdp"
                                        android:drawableTint="@color/bg_arrow_tint"
                                        android:layout_marginLeft="@dimen/_9sdp"
                                        android:layout_marginRight="@dimen/_9sdp"
                                        android:textColorHint="@color/txt_spinner_color"
                                        android:backgroundTint="@color/white"
                                        />
                                </LinearLayout>

                            </LinearLayout>
                        </LinearLayout>



                            <Button
                                android:id="@+id/btnConfirm"
                                android:layout_below="@+id/linContent"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_34sdp"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/_38sdp"
                                android:layout_marginEnd="@dimen/_38sdp"
                                android:layout_marginTop="@dimen/_25sdp"
                                android:layout_marginBottom="@dimen/_18sdp"
                                android:background="@drawable/curve_button"
                                android:padding= "@dimen/_5sdp"
                                android:textAllCaps="false"
                                android:visibility="gone"
                                android:text="@string/confirm"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_11sdp"/>
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    </RelativeLayout>

    <ImageView
        android:id="@+id/ivNext_"
        android:layout_width="@dimen/_45sdp"
        android:layout_height="@dimen/_45sdp"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        app:backgroundTint="@color/bg_floating_color"
        app:rippleColor="@color/white"
        android:src="@drawable/arrow_next"
        app:layout_anchor="@id/nestedScrollView"
        app:layout_anchorGravity="bottom|right|end" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>